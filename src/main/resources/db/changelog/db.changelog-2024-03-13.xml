<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="202403120" author="jwe">
        <createTable tableName="survey">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="surveyPK"/>
            </column>
            <column name="answer1" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="answer2" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="answer3" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
            <column name="surveyId" type="INT">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>